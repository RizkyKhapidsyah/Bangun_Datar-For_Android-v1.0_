<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_aplikasi_percobaan"
    tools:ignore="Deprecated" >

    <TextView
        android:id="@+id/TV_Judul_JajarGenjang_MC_HA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="10dp"
        android:layout_y="7dp"
        android:text="@+string/TV_Judul_JajarGenjang_MC_HA"
        android:textColor="#FFFFFFFF" />

    <TextView
        android:id="@+id/TV_SubJudul_JajarGenjang_MC_HA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="10dp"
        android:layout_y="33dp"
        android:text="@+string/TV_SubJudul_JajarGenjang_MC_HA"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <Button
        android:id="@+id/Button_Kembali_JajarGenjang_MC_HA"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_x="57dp"
        android:layout_y="372dp"
        android:background="@drawable/background_button_hitung"
        android:drawableLeft="@drawable/button_kembali"
        android:text="@+string/Button_Kembali"
        android:textSize="12sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/TV_Output_JajarGenjang_Alas_MC_HA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="58dp"
        android:layout_y="315dp"
        android:text="@string/TV_Output_JajarGenjang_Alas_MC_HA"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/ET_Output_JajarGenjang_Alas_MC_HA"
        android:layout_width="163dp"
        android:layout_height="22dp"
        android:layout_x="58dp"
        android:layout_y="334dp"
        android:background="@drawable/background_edittext_input_bs_1"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/TV_SatuanNilai_JajarGenjang_Alas_MC_HA"
        android:layout_width="62dp"
        android:layout_height="28dp"
        android:layout_x="224dp"
        android:layout_y="337dp"
        android:text="@+string/TV_SatuanNilai"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <Button
        android:id="@+id/Button_Hitung_JajarGenjang_MC_HA"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_x="57dp"
        android:layout_y="262dp"
        android:background="@drawable/background_button_hitung"
        android:drawableLeft="@drawable/button_hitung"
        android:text="@+string/Button_Hitung"
        android:textSize="12sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/Button_Reset_JajarGenjang_MC_HA"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_x="166dp"
        android:layout_y="262dp"
        android:background="@drawable/background_button_hitung"
        android:drawableLeft="@drawable/button_reset"
        android:text="@+string/Button_Reset"
        android:textSize="12sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/TV_Input_JajarGenjang_Tinggi_MC_HA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="58dp"
        android:layout_y="213dp"
        android:text="@+string/TV_Input_JajarGenjang_Tinggi_MC_HA"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/ET_Input_JajarGenjang_Tinggi_MC_HA"
        android:layout_width="163dp"
        android:layout_height="22dp"
        android:layout_x="58dp"
        android:layout_y="231dp"
        android:background="@drawable/background_edittext_input_bs_1"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/TV_Input_JajarGenjang_Luas_MC_HA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="58dp"
        android:layout_y="171dp"
        android:text="@+string/TV_Input_JajarGenjang_Luas_MC_HA"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/ET_Input_JajarGenjang_Luas_MC_HA"
        android:layout_width="163dp"
        android:layout_height="22dp"
        android:layout_x="58dp"
        android:layout_y="189dp"
        android:background="@drawable/background_edittext_input_bs_1"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textSize="12sp" >

        <requestFocus />
    </EditText>

    <TextView
        android:id="@+id/TV_SatuanNilai_JajarGenjang_Luas_MC_HA"
        android:layout_width="62dp"
        android:layout_height="28dp"
        android:layout_x="224dp"
        android:layout_y="192dp"
        android:text="@+string/TV_SatuanNilai_Pangkat2"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/TV_SatuanNilai_JajarGenjang_Tinggi_MC_HA"
        android:layout_width="62dp"
        android:layout_height="28dp"
        android:layout_x="224dp"
        android:layout_y="232dp"
        android:text="@+string/TV_SatuanNilai"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp" />

    <Button
        android:id="@+id/Button_Detail_JajarGenjang_MC_HA"
        android:layout_width="106dp"
        android:layout_height="wrap_content"
        android:layout_x="166dp"
        android:layout_y="372dp"
        android:background="@drawable/background_button_hitung"
        android:drawableLeft="@drawable/button_lihat_gambar"
        android:text="@+string/Button_LihatGambar"
        android:textSize="12sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/IV_Gambar_JajarGenjang_MC_HA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="7dp"
        android:layout_y="62dp"
        android:contentDescription="@+string/IV_Gambar_JajarGenjang_MC_HA"
        android:src="@drawable/jajar_genjang_besar2" />

    <Button
        android:id="@+id/Button_Rumus_JG_MC_HA"
        android:layout_width="106dp"
        android:layout_height="34dp"
        android:layout_x="197dp"
        android:layout_y="96dp"
        android:background="@drawable/background_button_hitung"
        android:drawableLeft="@drawable/button_lihat_rumus_form"
        android:text="@+string/Button_Rumus"
        android:textSize="12sp"
        android:textStyle="bold" />

</AbsoluteLayout>